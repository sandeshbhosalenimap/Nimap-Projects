@model IEnumerable<CategoryProduct.Models.Report>

@{
    ViewBag.Title = "ReportMethd";
}

<h2>ReportMethd</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CategoryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prise)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductCreatedBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CategoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prise)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductCreatedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductName)
        </td>
      
    </tr>
}

</table>
